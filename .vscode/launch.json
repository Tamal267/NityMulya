{
  "name": "Flutter Web Debug",
  "type": "dart",
  "request": "launch",
  "program": "lib/main.dart",
  "deviceId": "chrome",
  "args": [
    "--web-renderer",
    "html",
    "--web-port",
    "8080",
    "--web-hostname",
    "localhost"
  ],
  "env": {
    "FLUTTER_WEB_USE_SKIA": "false",
    "FLUTTER_WEB_AUTO_DETECT": "false"
  }
}